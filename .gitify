data_directory: _gitify/
backup_directory: _backup/
packages:
    modx.com:
        service_url: http://rest.modx.com/extras/
        packages:
            - pthumb
            - pdotools
            - collections
            - jolitypo
            - translit
            - updater
    modmore.com:
        service_url: https://rest.modmore.com/
        credential_file: '.modmore.com.key'
        packages:
            - contentblocks
            - redactor
data:
    system_settings:
        class: modSystemSetting
        primary: key
        where:
            - 'editedon:!=': '0000-00-00 00:00:00'
            - 'key:NOT IN':
                - 'gitifywatch.gitify_path'
                - 'gitify.git_path'
                - 'updater.last_send_core_notification'
                - 'formit.form_encryptkey'
            - 'xtype:!=': 'text-password'
    mediasources:
        class: sources.modMediaSource
        primary: id
        
    # mediasource_elements:
    #     class: sources.modMediaSourceElement
    #     primary:
    #         - source
    #         - object_class
    #         - object
    #         - context_key
    # contexts:
    #     class: modContext
    #     primary: key
    # context_settings:
    #     class: modContextSetting
    #     primary:
    #         - context_key
    #         - key
    
    categories:
        class: modCategory
        primary: category
        truncate_on_force:
            - modCategoryClosure
        where:
            - 'category': 'Layout'
    templates:
        class: modTemplate
        primary: templatename
        extension: .html
        
    # template_variables:
    #     class: modTemplateVar
    #     primary: name
    # template_variables_access:
    #     class: modTemplateVarTemplate
    #     primary:
    #         - tmplvarid
    #         - templateid
    
    chunks:
        class: modChunk
        primary: name
        extension: .html
        where:
            - 'category': '7'
    snippets:
        class: modSnippet
        primary: name
        extension: .php
        where:
            - 'category': '8'
    plugins:
        class: modPlugin
        primary: name
        extension: .php
        where:
            - 'category': '8'
    plugin_events:
        class: modPluginEvent
        primary:
            - pluginid
            - event
            
    # events:
    #     class: modEvent
