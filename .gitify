data_directory: _gitify/
backup_directory: _backup/
category_ids_in_elements: true
packages:
    modx.com:
        service_url: "https://rest.modx.com/extras/"
        packages:
            - collections
            - jolitypo
            - pdotools
            - pthumb
            - resizer
            - translit
            - versionx
    modmore.com:
        service_url: "https://rest.modmore.com/"
        credential_file: ".modmore.com.key"
        packages:
            - contentblocks
            - magicpreview
            - redactor
data:
    system_settings:
        class: modSystemSetting
        primary: key
        where:
            - { "editedon:!=": "0000-00-00 00:00:00" }
            - {
                  "key:NOT IN":
                      [
                          gitifywatch.gitify_path,
                          gitify.git_path,
                          updater.last_send_core_notification,
                          formit.form_encryptkey,
                      ],
              }
            - { "xtype:!=": text-password }
    mediasources:
        class: sources.modMediaSource
        primary: id
    access_media_source:
        class: sources.modAccessMediaSource
        primary: id
    categories:
        class: modCategory
        primary: category
        truncate_on_force:
            - modCategoryClosure
        where:
            - { category: Layout }
    templates:
        class: modTemplate
        primary: templatename
        extension: .html
    chunks:
        class: modChunk
        primary: id
        extension: .html
        where:
            - { "category:IN": [7, 9] }
    snippets:
        class: modSnippet
        primary: name
        extension: .php
        where:
            - { "category:IN": [9] }
    plugins:
        class: modPlugin
        primary: name
        extension: .php
        where:
            - { "category:IN": [9] }
    fc_sets:
        class: modFormCustomizationSet
        primary: id
    fc_profiles:
        class: modFormCustomizationProfile
        primary: id
    fc_profile_usergroups:
        class: modFormCustomizationProfileUserGroup
        primary:
            - usergroup
            - profile
    fc_action_dom:
        class: modActionDom
        primary:
            - set
            - name
    dashboards:
        class: modDashboard
        primary:
            - id
            - name
    dashboard_widget_placement:
        class: modDashboardWidgetPlacement
        primary:
            - dashboard
            - widget
    content_type:
        class: modContentType
        primary:
            - name
        where:
            - { "id:IN": [1] }
    user:
        class: modUser
        primary: id
        where:
            - { "id:IN": [2] }
    user_attributes:
        class: modUserProfile
        primary: internalKey
        where:
            - { "internalKey:IN": [2] }
        exclude_keys:
            - logincount
            - lastlogin
            - thislogin
            - sessionid
    user_member_groups:
        class: modUserGroupMember
        primary: id
        where:
            - { "member:IN": [2] }
    user_groups:
        class: modUserGroup
        primary: id
        where:
            - { "id:IN": [2] }
    access_policies:
        class: modAccessPolicy
        primary: id
        where:
            - { "id:IN": [17] }
    access_context:
        class: modAccessContext
        primary: id
        where:
            - { "id:IN": [11, 12] }
    cb_fields:
        class: cbField
        primary:
            - id
            - name
        package: contentblocks
    cb_categories:
        class: cbCategory
        primary:
            - id
            - name
    cb_layouts:
        class: cbLayout
        primary:
            - id
            - name
    cb_templates:
        class: cbTemplate
        primary:
            - id
            - name
    cb_defaults:
        class: cbDefault
        primary: id
    redactor_set:
        class: redConfigurationSet
        primary:
            - id
            - name
        package: redactor
