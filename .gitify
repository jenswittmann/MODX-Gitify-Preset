data_directory: _gitify/
backup_directory: _backup/
category_ids_in_elements: true
packages:
    modx.com:
        service_url: "https://rest.modx.com/extras/"
        packages:
            - collections
            - jolitypo
            - pdotools
            - translit
            - versionx
    modmore.com:
        service_url: "https://rest.modmore.com/"
        credential_file: ".modmore.com.key"
        packages:
            - contentblocks
            - magicpreview
            - redactor
data:
    system_settings:
        class: modSystemSetting
        primary: key
        where:
            - { "editedon:!=": "0000-00-00 00:00:00" }
            - {
                  "key:NOT IN":
                      [
                          gitifywatch.gitify_path,
                          gitify.git_path,
                          updater.last_send_core_notification,
                          formit.form_encryptkey,
                      ],
              }
            - { "xtype:!=": text-password }
        exclude_keys:
            - editedon
    mediasources:
        class: sources.modMediaSource
        primary: id
    access_media_source:
        class: sources.modAccessMediaSource
        primary: id
    # mediasource_elements:
    #     class: sources.modMediaSourceElement
    #     primary:
    #         - source
    #         - object_class
    #         - object
    #         - context_key
    # contexts:
    #     class: modContext
    #     primary: key
    # context_settings:
    #     class: modContextSetting
    #     primary:
    #         - context_key
    #         - key
    categories:
        class: modCategory
        primary: category
        truncate_on_force:
            - modCategoryClosure
        where:
            - { category: Layout }
    templates:
        class: modTemplate
        primary: templatename
        extension: .html
    # template_variables:
    #     class: modTemplateVar
    #     primary: name
    # template_variables_access:
    #     class: modTemplateVarTemplate
    #     primary:
    #         - tmplvarid
    #         - templateid
    chunks:
        class: modChunk
        primary: id
        extension: .html
        where:
            - { "category:IN": [5, 9] }
    snippets:
        class: modSnippet
        primary: name
        extension: .php
        where:
            - { "category:IN": [9] }
    plugins:
        class: modPlugin
        primary: name
        extension: .php
        where:
            - { "category:IN": [9] }
    plugin_events:
        class: modPluginEvent
        primary:
            - pluginid
            - event
        where:
            - { "pluginid:IN": [12] }
    fc_sets:
        class: modFormCustomizationSet
        primary: id
    fc_profiles:
        class: modFormCustomizationProfile
        primary: id
    fc_profile_usergroups:
        class: modFormCustomizationProfileUserGroup
        primary:
            - usergroup
            - profile
    fc_action_dom:
        class: modActionDom
        primary:
            - set
            - name
    dashboards:
        class: modDashboard
        primary:
            - id
            - name
    # dashboard_widgets:
    #     class: modDashboardWidget
    #     primary: id
    dashboard_widget_placement:
        class: modDashboardWidgetPlacement
        primary:
            - dashboard
            - widget
    content_type:
        class: modContentType
        primary:
            - name
        where:
            - { "id:IN": [1] }
    # menus:
    #     class: modMenu
    #     primary:
    #         - text
    #         - parent 
    user:
        class: modUser
        primary: id
        where:
            - { "id:IN": [2] }
    user_attributes:
        class: modUserProfile
        primary: internalKey
        where:
            - { "internalKey:IN": [2] }
        exclude_keys:
            - logincount
            - lastlogin
            - thislogin
            - sessionid
    user_member_groups:
        class: modUserGroupMember
        primary: id
        where:
            - { "member:IN": [2] }
    user_groups:
        class: modUserGroup
        primary: id
        where:
            - { "id:IN": [2] }
    # user_group_roles:
    #     class: modUserGroupRole
    #     primary: id
    access_policies:
        class: modAccessPolicy
        primary: id
        where:
            - { "id:IN": [17] }
    access_context:
        class: modAccessContext
        primary: id
        where:
            - { "id:IN": [11, 12] }
    # access_category:
    #     class: modAccessCategory
